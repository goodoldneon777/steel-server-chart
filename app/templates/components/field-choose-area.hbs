<div class="field-choose-select">
{{field-choose-select
	area = area
	selects = fieldChooseSelect
	name = "category"
	filterToggle = (action 'filterToggle')
	feed = (action 'feed')}}
</div>


<div class="field-choose-filter">
{{#if filterFlag}}
	{{field-choose-filter
		filters = fieldChooseFilter
		filterToggle = (action 'filterToggle')
		feed = (action 'feed')}}
{{/if}}
</div>

